         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <TextField fx:id="farmNameTextField" layoutX="235.0" layoutY="92.0" promptText="Farm Name" />
      <TextField fx:id="farmAddressTextField" layoutX="235.0" layoutY="160.0" promptText="Farm Address" />
      <TextField fx:id="farmPhoneTextField" layoutX="235.0" layoutY="240.0" promptText="Phone Number" />
      <Button fx:id="cleaDataButton" layoutX="144.0" layoutY="385.0" mnemonicParsing="false" onAction="#cleaDataButtonHandler" text="Clear Data">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
